<article class="pa4 black-80">
<h1>Add New Service</h1>
  <form action="/services" method="post" accept-charset="utf-8">
    <fieldset id="create-service-name" class="ba b--transparent ph0 mh0">
      <legend class="ph0 mh0 fw6 clip">Add Service Name</legend>
      <div class="mt3">
        <label class="db fw4 lh-copy f6" for="service_name">Service Name</label>
        <input class="pa2 input-reset ba bg-transparent w-100 measure" type="text" name="service_name"  id="service_name">
      </div>
      <div class="mt3">
        <label class="db fw4 lh-copy f6" for="expiry_date">Expiration Date:</label>
        <input class="b pa2 input-reset ba bg-transparent" type="date" name="expiry_date"  id="expiry_date">
      </div>
    </fieldset>
    <div class="mt3"><input class="b ph3 pv2 input-reset ba b--black bg-transparent grow pointer f6" type="submit" value="Add Service Name"></div>
  </form>
</article>
